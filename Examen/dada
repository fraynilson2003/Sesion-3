 if(sexo.equals(A) || sexo.equals(B) && años>=70){
      vacuna="C";
    }else if(sexo.equals(A) && años<=69 && años>=16){
      vacuna="A";
    }else if(sexo.equals(A) || sexo.equals(B) && años<16){
      vacuna="A";
    }else{
      if(sexo.equals(B) && años<=69 && años>=16){
        vacuna="B";
      }else{
        vacuna="Datos no validos, por favor vuelva a intertarlo";
      }
